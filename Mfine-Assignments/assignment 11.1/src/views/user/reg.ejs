        <hr/>
        <form action="/user/reg" method='post'>
        
            <div class='form-group'>
                <label for="name">name</label>
                <input class='form-control' type='text' name="name" id="name" placeholder="Enter your name">
            </div>

            <div >
            <div class="form-group">
                <label for="password">password</label>
